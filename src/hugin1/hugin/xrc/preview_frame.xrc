          <object class="sizeritem">
            <object class="wxCheckBox" name="preview_auto_cb">
              <label>Auto Preview</label>
              <tooltip>Automatically update preview on relevant changes</tooltip>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="preview_update_button">
              <label>Update</label>
              <tooltip>Update preview</tooltip>
            </object>
          </object>
        </object>
