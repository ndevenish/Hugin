            </object>
            <flag>wxEXPAND</flag>
          </object>
          <cols>2</cols>
          <rows>2</rows>
          <object class="sizeritem">
            <object class="wxSlider">
              <value></value>
              <min></min>
              <max></max>
              <style>wxSL_HORIZONTAL</style>
