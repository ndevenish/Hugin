<!DOCTYPE UI><UI version="3.1" stdsetdef="1">
<class>LensDialogBase</class>
<widget class="QDialog">
    <property name="name">
        <cstring>LensDialogBase</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>416</width>
            <height>300</height>
        </rect>
    </property>
    <property name="caption">
        <string>LensDialog</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QPushButton" row="0" column="1" rowspan="1" colspan="2">
            <property name="name">
                <cstring>loadLensButton</cstring>
            </property>
            <property name="enabled">
                <bool>false</bool>
            </property>
            <property name="text">
                <string>Load Lens</string>
            </property>
        </widget>
        <widget class="QPushButton" row="1" column="1" rowspan="1" colspan="2">
            <property name="name">
                <cstring>saveLensButton</cstring>
            </property>
            <property name="enabled">
                <bool>false</bool>
            </property>
            <property name="text">
                <string>Save Lens</string>
            </property>
        </widget>
        <spacer row="2" column="1">
            <property name="name">
                <cstring>spacer3</cstring>
            </property>
            <property name="orientation">
                <enum>Vertical</enum>
            </property>
            <property name="sizeType">
                <enum>Expanding</enum>
            </property>
            <property name="sizeHint">
                <size>
                    <width>20</width>
                    <height>140</height>
                </size>
            </property>
        </spacer>
        <widget class="QPushButton" row="3" column="2">
            <property name="name">
                <cstring>buttonCancel</cstring>
            </property>
            <property name="text">
                <string>&amp;Cancel</string>
            </property>
            <property name="accel">
                <string></string>
            </property>
            <property name="autoDefault">
                <bool>true</bool>
            </property>
        </widget>
        <widget class="QPushButton" row="3" column="1">
            <property name="name">
                <cstring>buttonOk</cstring>
            </property>
            <property name="text">
                <string>&amp;OK</string>
            </property>
            <property name="accel">
                <string></string>
            </property>
            <property name="autoDefault">
                <bool>true</bool>
            </property>
            <property name="default">
                <bool>true</bool>
            </property>
        </widget>
        <widget class="QGroupBox" row="0" column="0" rowspan="4" colspan="1">
            <property name="name">
                <cstring>groupBox1</cstring>
            </property>
            <property name="title">
                <string>Lens Settings</string>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QComboBox" row="0" column="1">
                    <item>
                        <property name="text">
                            <string>Rectiliniar (normal Lenses)</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Panoramic</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Circular Fisheye</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Full Frame Fisheye</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>Equirectangular</string>
                        </property>
                    </item>
                    <property name="name">
                        <cstring>lensTypeCombo</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="0" column="0">
                    <property name="name">
                        <cstring>textLabel1</cstring>
                    </property>
                    <property name="text">
                        <string>Lens Type(f):</string>
                    </property>
                </widget>
                <widget class="QLabel" row="2" column="0">
                    <property name="name">
                        <cstring>textLabel2_2</cstring>
                    </property>
                    <property name="text">
                        <string>Distortion(a):</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>DistortionAEdit</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit" row="2" column="1">
                    <property name="name">
                        <cstring>aEdit</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit" row="3" column="1">
                    <property name="name">
                        <cstring>bEdit</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="4" column="0">
                    <property name="name">
                        <cstring>textLabel2_2_3</cstring>
                    </property>
                    <property name="text">
                        <string>Distortion(c):</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>DistortionCEdit</cstring>
                    </property>
                </widget>
                <widget class="QLabel" row="3" column="0">
                    <property name="name">
                        <cstring>textLabel2_2_2</cstring>
                    </property>
                    <property name="text">
                        <string>Distortion(b):</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>DistortionBEdit</cstring>
                    </property>
                </widget>
                <widget class="QButtonGroup" row="1" column="0" rowspan="1" colspan="2">
                    <property name="name">
                        <cstring>buttonGroup1</cstring>
                    </property>
                    <property name="title">
                        <string>Horizontal Field of View / Focal Length</string>
                    </property>
                    <grid>
                        <property name="name">
                            <cstring>unnamed</cstring>
                        </property>
                        <widget class="QLineEdit" row="0" column="1">
                            <property name="name">
                                <cstring>HFOVEdit</cstring>
                            </property>
                        </widget>
                        <widget class="QLineEdit" row="1" column="1">
                            <property name="name">
                                <cstring>focalLengthEdit</cstring>
                            </property>
                            <property name="text">
                                <string>1</string>
                            </property>
                        </widget>
                        <widget class="QLineEdit" row="2" column="1">
                            <property name="name">
                                <cstring>focalLengthMultiplierEdit</cstring>
                            </property>
                        </widget>
                        <widget class="QLabel" row="2" column="0">
                            <property name="name">
                                <cstring>textLabel3</cstring>
                            </property>
                            <property name="text">
                                <string>Multiplier:</string>
                            </property>
                            <property name="buddy" stdset="0">
                                <cstring>FocalLengthMultiplier</cstring>
                            </property>
                        </widget>
                        <widget class="QLabel" row="3" column="0">
                            <property name="name">
                                <cstring>textLabel4</cstring>
                            </property>
                            <property name="text">
                                <string>35 mm equiv:</string>
                            </property>
                        </widget>
                        <widget class="QPushButton" row="4" column="0" rowspan="1" colspan="2">
                            <property name="name">
                                <cstring>exifButton</cstring>
                            </property>
                            <property name="text">
                                <string>Use Camera Information (EXIF)</string>
                            </property>
                            <property name="toolTip" stdset="0">
                                <string></string>
                            </property>
                            <property name="whatsThis" stdset="0">
                                <string>JPEG files created by many digital cameras contain the focal length used during taking the picture.</string>
                            </property>
                        </widget>
                        <widget class="QLabel" row="0" column="0">
                            <property name="name">
                                <cstring>textLabel1_2</cstring>
                            </property>
                            <property name="text">
                                <string>H. Field of View (Â°)</string>
                            </property>
                        </widget>
                        <widget class="QLabel" row="1" column="0">
                            <property name="name">
                                <cstring>textLabel2</cstring>
                            </property>
                            <property name="text">
                                <string>Focal Length (mm)</string>
                            </property>
                        </widget>
                        <widget class="QLabel" row="3" column="1">
                            <property name="name">
                                <cstring>focalLengthLabel</cstring>
                            </property>
                            <property name="text">
                                <string>0</string>
                            </property>
                            <property name="alignment">
                                <set>AlignVCenter|AlignRight</set>
                            </property>
                        </widget>
                    </grid>
                </widget>
                <widget class="QLineEdit" row="4" column="1">
                    <property name="name">
                        <cstring>cEdit</cstring>
                    </property>
                </widget>
            </grid>
        </widget>
    </grid>
</widget>
<connections>
    <connection>
        <sender>buttonCancel</sender>
        <signal>clicked()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>reject()</slot>
    </connection>
    <connection>
        <sender>buttonOk</sender>
        <signal>clicked()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>exifButton</sender>
        <signal>clicked()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>useEXIF()</slot>
    </connection>
    <connection>
        <sender>focalLengthEdit</sender>
        <signal>returnPressed()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>calculateHFOV()</slot>
    </connection>
    <connection>
        <sender>focalLengthEdit</sender>
        <signal>lostFocus()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>calculateHFOV()</slot>
    </connection>
    <connection>
        <sender>focalLengthMultiplierEdit</sender>
        <signal>returnPressed()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>calculateHFOV()</slot>
    </connection>
    <connection>
        <sender>focalLengthMultiplierEdit</sender>
        <signal>lostFocus()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>calculateHFOV()</slot>
    </connection>
    <connection>
        <sender>HFOVEdit</sender>
        <signal>lostFocus()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>calculateFocalLength()</slot>
    </connection>
    <connection>
        <sender>HFOVEdit</sender>
        <signal>returnPressed()</signal>
        <receiver>LensDialogBase</receiver>
        <slot>calculateFocalLength()</slot>
    </connection>
</connections>
<slots>
    <slot specifier="pure virtual">useEXIF()</slot>
    <slot specifier="pure virtual">calculateFocalLength()</slot>
    <slot specifier="pure virtual">calculateHFOV()</slot>
</slots>
<pixmapinproject/>
<layoutdefaults spacing="6" margin="11"/>
</UI>
